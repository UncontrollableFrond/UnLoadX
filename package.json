{
  "name": "unload",
  "version": "1.0.0",
  "description": "Load balancing and testing tool with UI measuring performance",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-client && npm run test-server",
    "test-client": "mocha spec/clientspec.js",
    "test-server": "mocha spec/serverspec.js",
    "start": "npm run startsiege | npm run startserver",
    "startsiege": "nodemon siege-service/server.js --exec babel-node --presets es2015",
    "startserver": "nodemon server/server.js --exec babel-node --presets es2015 && tsc && concurrently \"npm run tsc:w\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabithablagdon/UnLoadX.git"
  },
  "author": "Andrew Brown, James Ramadan, Tabitha Blagdon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tabithablagdon/UnLoadX/issues"
  },
  "homepage": "https://github.com/tabithablagdon/UnLoadX#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-alpha.8",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.14",
    "angular2-jwt": "^0.1.18",
    "async": "^2.0.1",
    "auth0-lock": "^10.1.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "core-js": "^2.4.0",
    "d3": "^3.5.17",
    "express": "^4.14.0",
    "mocha": "^3.0.0",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "ng2-nvd3": "^1.1.3",
    "nodemon": "^1.10.0",
    "pg": "^6.0.3",
    "reflect-metadata": "^0.1.3",
    "request": "^2.74.0",
    "rxjs": "5.0.0-beta.6",
    "sequelize": "^3.23.6",
    "siege": "^0.2.0",
    "socket.io": "^1.4.8",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "chai-http": "^3.0.0",
    "concurrently": "^2.0.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
